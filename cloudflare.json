{

    "name": "jsDelivr CDN",
    "description": "Load popular JS libraries in your websites using the huge global network of jsDelivr CDN",
    "version": "0.2.3",
    "category": "library",
    "main": "public/javascripts/jsdelivr.js",
    "config": {
        "details": {
            "Language": "English",
            "Price": "Free",
            "Restrictions": "None",
            "TOS": "http://www.cloudflare.com/terms",
            "has_js": true
        },
        "assets": {
            "logos": {
                "200px": "./public/images/logo-200.png",
                "132px": "./public/images/logo-132.png"
            }
        },
        "interface": [{
            "type": "string",
            "id": "libraries",
            "name": "JavaScript Libraries",
            "description": "Please add your libraries here",
            "domain_request": true,
            "private": false
        }, {
            "type": "select",
            "id": "combined",
            "name": "Combine libraries",
            "description": "Choose whether to load your libraries in a single bundled request or as individual requests (one per file)",
            "domain_request": true,
            "private": false,
            "options": [{
                "label": "Yes",
                "value": "true"
            }, {
                "label": "No",
                "value": ""
            }]
        }, {
            "type": "select",
            "id": "cedexis",
            "name": "Cedexis",
            "description": "Choose whether to contribute to Cedexis stats",
            "domain_request": true,
            "private": false,
            "options": [{
                "label": "Yes",
                "value": "true"
            }, {
                "label": "No",
                "value": ""
            }]
        }]
    }
}
